{
  "version": 2,
  "buildCommand": "npm run build",
  "public": "dist/spa",
  "env": {
    "NODE_ENV": "production"
  },
  "routes": [
    {
      "src": "^/api/(.*)",
      "dest": "/api/index.ts?route=$1"
    },
    {
      "src": "^/(.*\\..*)",
      "dest": "/dist/spa/$1"
    },
    {
      "src": "^/(?!api)(.*)$",
      "dest": "/dist/spa/index.html"
    }
  ]
}
