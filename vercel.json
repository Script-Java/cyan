{
  "version": 2,
  "buildCommand": "npm run build",
  "env": {
    "NODE_ENV": "production"
  },
  "routes": [
    {
      "src": "^/dist/spa/(.*)",
      "dest": "/dist/spa/$1"
    },
    {
      "src": "^/api/(.*)",
      "dest": "/api/[...route]"
    },
    {
      "src": "^/(?!api).*$",
      "dest": "/dist/spa/index.html"
    }
  ]
}
